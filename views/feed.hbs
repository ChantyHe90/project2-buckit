<h1>Hi {{user.username}}!</h1>
<h4>Check out the community's latest experiences:</h4>
<br>
<div class="row">
    <div class="col-sm-6">
        <div class="card border-dark mb-3" style="max-width: 18rem;">
            {{#each experiences}}
            <div class="card-body">
                <div class="card-header">Experience</div>

                <h5 class="card-title">Title: {{this.title}}</h5>
                <p class="card-text">The Plan:{{this.plan}}</p>
                <a href="#" class="btn btn-primary">Maybe Comment Button?</a>

            </div>
            {{/each}}
        </div>
    </div>

    <div class="row">
        <div class="col-sm-6">
            <div class="card border-dark mb-3" style="max-width: 18rem;">
                <div class="card-header">Location</div>
                <div class="card-body text-dark">

                    {{#each locations}}
                    <h5 class="card-title">Title: {{this.title}}</h5>
                    <p class="card-text">The Plan:{{this.plan}} <br>
                        <p>Due Date: {{this.expireDate}}</p>
                    </p>
                    <a href="#" class="btn btn-primary">Maybe Comment Button?</a>
                    {{/each}}
                </div>
            </div>
        </div>

